<label>Define the question</label>
<div data-partial='text'></div>

<label>Define the answers</label>
<ol class='choices'>
<% model.get('choice_endpoints').each(function(choice) { %>
  <li
   class='choice'
   data-endpoint-id="<%= choice.get('uuid') %>">
    <input type='text' value="<%= choice.get('label') %>">

    <i class="icon-info-sign"></i>
    
    <i class='close remove'>&times;</i>

  </li>
<% }); %>
</ol>

<button class='btn btn-primary new-choice'>Add another answer</button>
